{
  "extends": ["airbnb-base"],
  "env": {
    "node": true,
    "es2022": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "import/extensions": ["error", "ignorePackages"],
    "no-underscore-dangle": ["error", { "allow": ["_id"] }]
  }
}
